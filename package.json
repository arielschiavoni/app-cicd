    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-c8": "0.30.1",
    "@vitest/ui": "0.30.1",
    "c8": "7.13.0",
    "happy-dom": "8.9.0",
    "msw": "0.49.3",
    "prettier": "2.8.7",
    "rollup-plugin-analyzer": "4.0.0",
