    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react": "2.2.0",
    "@vitest/coverage-c8": "0.26.3",
    "@vitest/ui": "0.26.3",
    "c8": "7.12.0",
    "happy-dom": "8.1.2",
    "msw": "0.49.2",
    "prettier": "2.8.1",
    "rollup-plugin-analyzer": "4.0.0",
    "rollup-plugin-visualizer": "5.9.0",
